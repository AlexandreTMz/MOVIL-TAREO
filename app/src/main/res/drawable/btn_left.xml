<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
<item android:state_pressed="false">
    <layer-list>
        <item>
            <shape>
                <solid android:color="@android:color/darker_gray" />
                <corners android:topRightRadius="10dip" />
            </shape>
        </item>
        <item>
            <shape>
                <solid android:color="@color/colorCancel"></solid>
                <corners android:topRightRadius="10dip" />
            </shape>
        </item>
    </layer-list>
</item>

<item android:state_pressed="true">
    <layer-list>
        <item>
            <shape>
                <stroke android:color="@color/colorCancelDarck" android:width="5dp"></stroke>
                <solid android:color="#102746" />
                <corners android:topRightRadius="10dip" />
            </shape>
        </item>
        <item>
            <shape>
                <solid android:color="@color/colorCancelDarck"></solid>
                <corners android:topRightRadius="10dip" />
            </shape>
        </item>
    </layer-list>
</item>
</selector>